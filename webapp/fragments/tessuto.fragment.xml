<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="tessutoSelectDialogId"
		title="Tessuto"
		items="{tessuto>/}"
		search=".onValueHelptessutoSearch"
		confirm=".onValueHelptessutoClose"
		cancel=".onValueHelptessutoClose">
		<StandardListItem
			icon="sap-icon://product"
			title="{tessuto>MATNR}"
            iconDensityAware="true"
			iconInset="false"
            iconActive="true"
            key="{tessuto>MATNR}"
			description="{tessuto>MAKTX}" />
	</SelectDialog>
</core:FragmentDefinition>